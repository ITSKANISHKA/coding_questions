class Solution {
    void pushZerosToEnd(int[] arr) {
        // code here
        int num=0;
        for(int i=0;i<arr.length;i++){
            if(arr[i]!=0){
                arr[num]=arr[i];
                num++;
            }
        }
      while(num<arr.length){
            arr[num]=0;
            num++;
        }
    }
}
